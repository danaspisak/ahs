<p id="notice"><%= notice %></p>

<p>
  <strong>ID:</strong>
  <%= @interview.id %>
  <% @interview.responses.each do |r| %>
    <%= r.question.content %>
    <% if r.question.response_type.eql? "SelectionResponse" %>
      <%= r.selection %>
    <% elsif r.question.response_type.eql? "TextResponse" %>
      <%= r.content %>
    <% end %>
  <% end %>
</p>
