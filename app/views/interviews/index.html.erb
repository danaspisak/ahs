<p id="notice"><%= notice %></p>

<h1>Listing Interviews</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Responses</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interviews.each do |interview| %>
      <tr>
        <td><%= interview.id %>
        <td><%= interview.responses.count %></td>
        <td><%= link_to 'Show', interview %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
